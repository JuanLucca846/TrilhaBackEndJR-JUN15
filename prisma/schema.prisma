generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User{
  id Int @id @default(autoincrement())
  name String 
  email String @unique
  password String
  task Task[]
}

model Task{
  id Int @id @default(autoincrement())
  title String
  description String
  status String
  userId Int
  user User? @relation(fields: [userId], references: [id])
}